class Solution {
    public int[] twoSum(int[] nums, int target) {
        
        int res =0;
        int[] good_nums = new int[2];
        for(int i =0;i<nums.length;i++)
        {
            for(int y = i+1;y<nums.length;y++)
            {
                if(nums[i] +nums[y] ==target)
                {
                    good_nums[0] = i;
                    good_nums[1] = y;
                }
            }
        }
        return good_nums;
    }
}
complexity level:O(nÂ²)
