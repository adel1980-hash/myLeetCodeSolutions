class Solution {
    public int removeElement(int[] nums, int val) {

        ArrayList<Integer> sortedNums = new ArrayList<>();
        int k =0;
        int temp =0;
        int res = 0;
      for(int i =0;i<nums.length; i++)
      {
                    temp = nums[i];
                    

       if(temp!=val)
       {
        nums[k] = temp;
        k++;
       }        
        
      }  
      res =k;

      return res;

    }
}
